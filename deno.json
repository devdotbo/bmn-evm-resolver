    "order:create": "deno run --allow-net --allow-env --allow-read --allow-write --unstable-kv --env-file=.env scripts/create-order.ts",
    "decode:factory": "deno run --allow-net --allow-env --allow-read scripts/decode-factory-events.ts",
    "human2abi": "deno run --allow-read scripts/human2abi.ts",
    "abis:sync": "deno run --allow-read --allow-env --allow-write scripts/sync-abis.ts",
    "ponder:sync": "deno run --allow-read --allow-env --allow-write scripts/sync-ponder-schema.ts"
